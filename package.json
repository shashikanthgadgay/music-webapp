{
  "name": "music-webapp",
  "version": "1.0.0",
  "description": "Music Web App",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "eslint --ext .js --ext .jsx ./src ./test",
    "eslintFix": "eslint --ext .js --ext .jsx ./src ./test --fix",
    "start": "npm run build && node server.js",
    "dev": "webpack-dev-server --hot --inline",
    "copy": "./node_modules/copyfiles/copyfiles -f ./index.html ./dist",
    "build": "npm run copy && ./node_modules/webpack/bin/webpack.js -p --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shashikanthgadgay/music-webapp.git"
  },
  "author": "shashikanth",
  "license": "ISC",
  "dependencies": {
    "express": "^4.14.0",
    "history": "^2.1.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-router": "^2.6.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-env": "0.0.3",
    "babel-register": "^6.24.1",
    "babel-watch": "^2.0.6",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-jsx-control-statements": "^2.1.1",
    "eslint-plugin-react": "^5.2.2",
    "fetch-mock": "^5.12.0",
    "file-loader": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "jsdom": "^11.0.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.0",
    "karma-jsdom-launcher": "^3.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "karma-webpack-with-fast-source-maps": "^1.10.0",
    "mocha": "^2.5.3",
    "sinon": "^2.3.5",
    "style-loader": "^0.18.2",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
